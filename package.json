{
  "name": "kolable-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "typegen": "apollo client:codegen ./src/types.d.ts --outputFlat --target typescript",
    "format:organize-imports": "organize-imports-cli ./src/**/*.ts[x]",
    "format:prettier": "prettier --write ./src/**/*.ts[x]",
    "format": "npm run format:organize-imports && npm run format:prettier"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "antd": "3.26.6",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "aws-sdk": "^2.600.0",
    "axios": "^0.19.1",
    "braft-editor": "^2.3.8",
    "coupon-code": "^0.4.5",
    "fingerprintjs2": "^2.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.10.3",
    "mustache": "^3.2.1",
    "node-sass": "^4.13.0",
    "query-string": "^6.9.0",
    "ramda": "^0.26.1",
    "rc-slider": "^8.7.1",
    "rc-tooltip": "^4.0.0-alpha.3",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-facebook-pixel": "^0.1.3",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-messenger-customer-chat": "^0.7.2",
    "react-player": "1.14.2",
    "react-responsive": "^8.0.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-scroll": "^1.7.15",
    "react-social-login": "^3.4.6",
    "react-sortablejs": "^1.5.1",
    "react-svg": "^11.0.6",
    "schema-dts": "^0.4.5",
    "sortablejs": "^1.10.2",
    "styled-components": "^4.4.1",
    "styled-system": "^5.1.4",
    "throttle-typescript": "^1.0.1",
    "use-query-params": "^0.5.0",
    "use-react-router": "^1.0.7",
    "uuid": "^3.3.3",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@types/braft-editor": "^2.2.0",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mustache": "^0.8.32",
    "@types/node": "^13.1.5",
    "@types/ramda": "^0.26.39",
    "@types/rc-slider": "^8.6.5",
    "@types/react": "^16.9.17",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-scroll": "^1.5.4",
    "@types/styled-components": "^4.4.2",
    "@types/uuid": "^3.4.6",
    "@types/yup": "^0.26.27",
    "apollo": "^2.21.2",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "husky": ">=1",
    "less-loader": "^5.0.0",
    "lint-staged": ">=8",
    "organize-imports-cli": "^0.7.0",
    "prettier": "^1.19.1",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.5",
    "react-hot-loader": "^4.12.18",
    "source-map-explorer": "^2.2.1",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "organize-imports-cli",
      "prettier --write",
      "git add"
    ]
  }
}

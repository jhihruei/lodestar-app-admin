    "@hot-loader/react-dom": "^17.0.1",
    "@types/audiobuffer-to-wav": "^1.0.0",
    "@types/braft-editor": "^2.2.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.3.0",

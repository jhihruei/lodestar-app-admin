{
  "name": "kolable-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "CI=false react-app-rewired build",
    "test": "react-scripts test",
    "typegen": "apollo client:codegen ./src/types.d.ts --outputFlat --target typescript",
    "format:organize-imports": "organize-imports-cli ./src/**/*.ts[x]",
    "format:prettier": "prettier --write ./src/**/*.ts[x]",
    "format": "npm run format:organize-imports && npm run format:prettier",
    "trans:compile": "tsc --noEmit false --outDir src/extracted",
    "trans:extract": "babel --config-file ./.intl.babelrc ./src/extracted > /dev/null",
    "trans:manage": "node manageTranslations.js",
    "trans:clean": "rm -rf ./src/extracted",
    "trans": "npm run trans:clean && npm run trans:compile && npm run trans:extract && npm run trans:manage && npm run trans:clean"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "antd": "3",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "aws-sdk": "^2.680.0",
    "axios": "^0.19.2",
    "braft-editor": "^2.3.9",
    "coupon-code": "^0.4.5",
    "fingerprintjs2": "^2.1.0",
    "graphql-tag": "^2.10.3",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.11.2",
    "moment-timezone": "^0.5.31",
    "mustache": "^4.0.1",
    "node-sass": "^4.14.1",
    "query-string": "^6.12.1",
    "ramda": "^0.27.0",
    "rc-slider": "^9.2.4",
    "rc-tooltip": "^4.2.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-facebook-pixel": "^0.1.3",
    "react-ga": "^2.7.0",
    "react-helmet": "^6.0.0",
    "react-intl": "^4.6.4",
    "react-messenger-customer-chat": "^0.7.2",
    "react-player": "^2.1.1",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-scroll": "^1.7.16",
    "react-social-login": "^3.4.7",
    "react-sortablejs": "^2.0.11",
    "react-svg": "^11.0.24",
    "schema-dts": "^0.6.0",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "throttle-typescript": "^1.0.1",
    "use-query-params": "^1.1.3",
    "use-react-router": "^1.0.7",
    "uuid": "^8.1.0",
    "yup": "0.28"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/braft-editor": "^2.2.0",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/mustache": "^4.0.1",
    "@types/node": "^14.0.10",
    "@types/ramda": "^0.27.6",
    "@types/rc-slider": "^8.6.5",
    "@types/react": "^16.9.35",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-scroll": "^1.5.5",
    "@types/styled-components": "^5.1.0",
    "@types/uuid": "^8.0.0",
    "@types/yup": "0.28",
    "apollo": "^2.28.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-react-intl": "^7.5.16",
    "customize-cra": "^1.0.0",
    "husky": "^4.2.5",
    "less-loader": "5",
    "lint-staged": "^10.2.8",
    "organize-imports-cli": "^0.7.0",
    "prettier": "^2.0.5",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.6",
    "react-hot-loader": "^4.12.21",
    "react-intl-translations-manager": "^5.0.3",
    "source-map-explorer": "^2.4.2",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "organize-imports-cli",
      "prettier --write",
      "git add"
    ]
  }
}
